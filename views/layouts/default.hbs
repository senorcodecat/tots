<body ng-app="tots" ng-controller="app" {{#if @user_profile.settings.nightmode}}class="nightmode"{{/if}}>


    <header id="header">
        <div class="wrapper">
            <nav ng-if="ui.history.length>1">
                <ul>
                    <li><a ng-click="popHistory()">‚¨Ö {% backTitle() %}</a></li>
                </ul>
            </nav>
            <nav class="right" ng-if="ui.history.length>1">
                <ul>
                    <li><a href="/">‚ùé</a></li>
                </ul>
            </nav>

            <nav ng-if="!ui.history || ui.history.length<=1">
                <ul>
                    <li class="ng-class:{active: ui.nav=='home'};"><a href="/">üè†<span class="label"> Home</span></a></li>
                    {{#@loggedin}}
                    <li class="ng-class:{active: ui.nav=='notifications'};"><a href="/notifications">üéâ<span class="label"> Notifications</span></a></li>
                    <li class="ng-class:{active: ui.nav=='public'};"><a href="/public">üåé<span class="label"> Public</span></a></li>
                    {{/@loggedin}}
                    <li class="ng-class:{active: ui.nav=='search'}; small_screen_only"><a href="/search">üîç<span class="label"> Search</span></a></li>

                </ul>
            </nav>
            <nav class="right" ng-if="!ui.history || ui.history.length<=1">
                <ul>
                    <li class="big_screen_only"><form method="get" action="/search"><input type="search" name="query" placeholder="Search" /></form></li>
                    {{#@loggedin}}<li class="menu">
                        <a href="/me" target="_self">Me</a>
                        <ul>
                            <li><A href="/me" target="_self">My Profile</a></li>
                            <li><a href="/editprofile">Edit Profile</a></li>
                            <li><a href="/logout" target="_self">Logout</a></li>
                        </ul>
                    </li>{{/@loggedin}}
                    {{^@loggedin}}<li><a href="/login" target="_self">Login</a></li>{{/@loggedin}}
                </ul>
            </nav>
        </div>
    </header>
    <div class="body">
        <div ng-view>
          <!-- Loading Tots! -->
        </div>
        {{{body}}}
    </div>
    <div class="ng-class:{active: ui.overlay}; overlay" ng-click="ui.overlay=false">
        <div class="lightbox" style="background-image: url('{% ui.lightbox_img %}')"></div>
    </div>

      <script type="text/ng-template" id="/people-mentions.tpl">
          <ul class="list-group user-search">
              <li mentio-menu-item="person" ng-repeat="person in items" class="list-group-item">
                  <span class="text-primary"
                      ng-bind-html="person.displayName | mentioHighlight:typedTerm:'menu-highlighted' | unsafe"></span>
              </li>
          </ul>
      </script>

</body>
