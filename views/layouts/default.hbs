<html>
<head>
    <title>{{#if post}}{{post.text}} | Tots.cool{{else}}Tots.cool{{/if}}</title>
    <base href="/">
    <script>
      {{#if @loggedin}}
      var auth = {{{@payload}}};
      {{else}}
      var auth = false;
      {{/if}}
    </script>
    {{#if post}}
    <!-‚Äî facebook open graph tags -->
    <meta property="og:type" content="website" />
    <meta property="og:url" content="{{@thisurl}}" />
    <meta property="og:title" content="Tot from {{post.user.displayName}}" />
    <meta property="og:description" content="{{post.text}}" />
    {{#post.images}}
        <meta property="og:image" content="{{url}}" />
    {{/post.images}}

    <!-‚Äî twitter card tags additive with the og: tags -->
    <meta name="twitter:domain" value="tots.cool" />
    <meta name="twitter:title" value="Tot from {{post.user.displayName}}" />
    <meta name="twitter:description" value="{{post.text}}" />
    {{#post.images}}
        <meta name="twitter:card" content="summary_large_image">
        <meta name="twitter:image" content="{{url}}" />
    {{/post.images}}
    {{^post.images}}
      <meta name="twitter:card" content="summary" />
    {{/post.images}}
    <meta name="twitter:url" value="{{@thisurl}}" />
    {{/if}}


